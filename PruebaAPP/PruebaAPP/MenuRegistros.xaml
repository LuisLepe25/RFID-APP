<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PruebaAPP.MenuRegistros"
             Title="Entradas y salidas">
    <ContentPage.Content>
        <StackLayout>
            <Button x:Name="btnEntradas" Text="Ver entradas" Clicked="btnEntradas_Clicked"/>
            <Button x:Name="btnSalidad" Text="Ver salidad" Clicked="btnSalidad_Clicked"/>
            <Button x:Name="btnLogsUsuario" Text="Ver movimientos por usuario" Clicked="btnLogsUsuario_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>